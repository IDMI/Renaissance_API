component	output="false"{	void function index(event, rc, prc)		output="false"	{		setNextEvent(getSetting('DefaultEvent'));	}	void function onAppInit(event, rc, prc)		output="false"	{		// load application constants		structInsert(application, "constants", deserializeJSON(fileRead(expandPath("/config/Application.json"))), true);	}	void function onRequestStart(event, rc, prc)		output="false"	{		if (structKeyExists(url, "ormreload")) ormReload();	}	void function onRequestEnd(event, rc, prc)		output="false"	{	}	void function onSessionStart(event, rc, prc)		output="false"	{	}	void function onSessionEnd(event, rc, prc)		output="false"	{		var sessionScope = event.getValue("sessionReference");		var applicationScope = event.getValue("applicationReference");	}	void function onException(event, rc, prc)		output="false"	{		//Grab Exception From request collection, placed by ColdBox		var exceptionBean = event.getValue("ExceptionBean");	}	void function onInvalidEvent(event, rc, prc)		output="false"	{		setNextEvent("HTMLStatus._404");	}	void function onMissingTemplate(event, rc, prc)		output="false"	{		//Grab missingTemplate From request collection, placed by ColdBox		var missingTemplate = event.getValue("missingTemplate");	}}